@model List<Programmer_Adda.Models.Enquiry>

@{
    ViewBag.Title = "EnquiryMgmt";
    Layout = "~/Views/Shared/AdminMaster.cshtml";
}

<div class="row" style="font-size:25px;">
    <h2 style="color:orange;font-family:Gabriola;font-size:50px;text-align:center;font-weight:bold">EnquiryMgmt</h2>

    <table class="table table-responsive">
        <tr>
            <th>
                Name
            </th>
            <th>
                Email
            </th>
            <th>
                Mobile
            </th>
            <th>
                Message
            </th>
            <th>
                Enquiry Date
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mob)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.msg)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.edt)
                </td>

            </tr>
        }

    </table>
    </div>
